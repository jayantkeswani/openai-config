{
  "currency": "ZAR",
  "defs": {
    "NUMBER": "\\d+([,.]\\d+)?"
  },
  "price": {
    "template": "(R?%{NUMBER:rand})",
    "scale": {
      "rand": 1
    }
  },
  "pricePerUnit": {
    "templates": [
      "%{PRICE:price}/(%{NUMBER:count})? ?%{UNIT:unit}"
    ]
  },
  "netQuantity": {
    "units": [
      {
        "regex": "kg",
        "scale": 1,
        "type": "KG"
      },
      {
        "regex": "g",
        "scale": 0.001,
        "type": "KG"
      },
      {
        "regex": "l",
        "scale": 1,
        "type": "L"
      },
      {
        "regex": "ml",
        "scale": 0.001,
        "type": "L"
      },
      {
        "type": "UNIT",
        "regex": "|",
        "scale": 1
      }
    ],
    "parsers": [
      {
        "template": "%{NUMBER:count} ?%{UNIT:unit} ?x ?%{NUMBER:multiplier}",
        "logic": {
          "type": "unitCount"
        }
      },
      {
        "template": "%{NUMBER:count} ?%{UNIT:unit}",
        "logic": {
          "type": "unitCount"
        }
      }
    ]
  },
  "promotion": {
    "parsers": [
      
      {
        "template": "Smart Price: %{PRICE:price}",
        "logic": {
          "newPrice": {}
        },
        "fields": {
          "originalPrice": "IGNORE_DISCOUNT",
          "promotionPricePerUnit": "SUBTRACT_DISCOUNT",
          "promotionPrice": "INVERSE_DISCOUNT"
        },
        "type": "PRICE_CUT"
      },
      {
        "template": "Was %{PRICE:price}",
        "logic": {
          "oldPrice": {}
        },
        "type": "PRICE_CUT"
      },
      {
        "template": "Buy %{NUMBER:count} For %{PRICE:price}",
        "logic": {
          "countForPrice": {}
        },
        "type": "MULTI_BUY"
      },
      {
        "template": "Buy Any %{NUMBER:count} For %{PRICE:price}",
        "logic": {
          "countForPrice": {}
        },
        "type": "MULTI_BUY"
      },
      {
        "template": "Click For Incredible Freebie",
        "logic": {
          "none": {}
        },
        "type": "COUPON"
      },
      {
        "template": "Incredible Freebie:",
        "logic": {
          "none": {}
        },
        "type": "COUPON"
      }
    ]
  }
}