{
  "metadata": {
    "retailer": "PNP-ZA",
  "country": "ZA",
  "language": "en",
  "pageType": "store",
  "x-browser": "chrome"
  },
  "linkSelectors": [
    {
      "name": "productLink",
      "query": ".item > a",
      "attr": "abs:href",
      "metadata": {
        "pageType": "detail"
      }
    },
    {
      "name": "nextPageLink",
      "query": ".pagination-next a",
      "attr": "abs:href",
      "metadata": {
        "pageType": "store"
      }
    }
  ],
  "resourceSelectors": [
    {
      "name": "productImage",
      "query": ".thumb img",
      "attr": "abs:src"
    }
  ],
  "capture": {
    "screenshot": {
     "enabled": true,
     "options": {
      "format": "jpeg",
      "quality": 25,
      "fullPage": true,
      "fromSurface": true,
      "captureBeyondViewport": false
     }
    }
  },
  "script": {
    "file": "pnp-za-list.groovy"
  }
}